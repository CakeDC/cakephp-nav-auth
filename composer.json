{
    "name": "cakedc/cakephp-nav-auth",
    "description": "CakeDC Navision Authentication plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4",
        "queencitycodefactory/cakesoap": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\NavAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\NavAuth\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
